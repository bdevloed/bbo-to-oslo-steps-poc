###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#LinkEvent
:LinkEvent rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Event
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :has_eventDefinition ;
                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass :LinkEventDefinition
                                                      ]
