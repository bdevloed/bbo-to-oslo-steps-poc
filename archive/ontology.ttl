###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#Activity
:Activity rdf:type owl:Class ;
          rdfs:subClassOf :FlowNode ,
                          [ rdf:type owl:Class ;
                            owl:complementOf [ rdf:type owl:Restriction ;
                                               owl:onProperty :has_outgoing ;
                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                               owl:onClass :ConditionalSequenceFlow
                                             ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :has_default ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :SequenceFlow
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :has_ioSpecification ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :InputOutputSpecification
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :has_loopCharacteristics ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :LoopCharacteristics
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :takesPlaceAt ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :ManufacturingFacility
                          ] ;
          owl:disjointWith :Event ,
                           :Gateway ;
          rdfs:comment "Work that a company or organization performs using business processes. An activity can be atomic or non-atomic (compound). The types of activities that are a part of a Process Model are: Process, Sub-Process, and Task."@en ;
          rdfs:label "Activity"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#AdHocSubProcess
:AdHocSubProcess rdf:type owl:Class ;
                 rdfs:subClassOf :SubProcess ,
                                 [ rdf:type owl:Class ;
                                   owl:complementOf [ rdf:type owl:Restriction ;
                                                      owl:onProperty :has_flowElements ;
                                                      owl:someValuesFrom [ rdf:type owl:Class ;
                                                                           owl:unionOf ( :EndEvent
                                                                                         :StartEvent
                                                                                       )
                                                                         ]
                                                    ]
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :has_flowElements ;
                                   owl:someValuesFrom :Activity
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :has_completionCondition ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass :Expression
                                 ] ;
                 rdfs:comment "An Ad-Hoc Sub-Process is a specialized type of Sub-Process that is a group of Activities that have no REQUIRED sequence relationships. A set of Activities can be defined for the Process, but the sequence and number of performances for the Activities is determined by the performers of the Activities."@en ;
                 rdfs:label "AdHocSubProcess"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#Agent
:Agent rdf:type owl:Class ;
       rdfs:comment "Person or a Software that performs an activity"@en ;
       rdfs:label "Agent" .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#AllMultiInstanceLoopCharacteristics
:AllMultiInstanceLoopCharacteristics rdf:type owl:Class ;
                                     rdfs:subClassOf :MultiInstanceLoopCharacteristics ;
                                     rdfs:comment """All: no Event is ever thrown, a token is produced after completion of all instances.
Any thrown Events can be caught by boundary Events on the Multi-Instance Activity."""@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#BoundaryEvent
:BoundaryEvent rdf:type owl:Class ;
               rdfs:subClassOf :CatchEvent ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :is_attachedToRef ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :Activity
                               ] .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#BusinessRuleTask
:BusinessRuleTask rdf:type owl:Class ;
                  rdfs:subClassOf :Task ;
                  rdfs:comment "A Business Rule Task provides a mechanism for the Process to provide input to a Business Rules Engine and to get the output of calculations that the Business Rules Engine might provide. The InputOutputSpecification of the Task (see page 211) will allow the Process to send data to and receive data from the Business Rules Engine."@en ;
                  rdfs:label "BusinessRuleTask"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#CallActivity
:CallActivity rdf:type owl:Class ;
              rdfs:subClassOf :Activity ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :has_calledElement ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :CallableElement
                              ] ;
              rdfs:comment "A Call Activity identifies a point in the Process where a global Process or a Global Task is used. The Call Activity acts as a ‘wrapper’ for the invocation of a global Process or Global Task within the execution. The activation of a call Activity results in the transfer of control to the called global Process or Global Task."@en ;
              rdfs:label "CallActivity"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#CallableElement
:CallableElement rdf:type owl:Class ;
                 rdfs:subClassOf :RootElement ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :has_ioSpecification ;
                                   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass :InputOutputSpecification
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :name ;
                                   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onDataRange xsd:string
                                 ] ;
                 rdfs:comment "CallableElement is the abstract super class of all Activities that have been defined outside of a Process or Choreography but which can be called (or reused), by a Call Activity, from within a Process or Choreography. It MAY reference Interfaces that define the service operations that it provides. The BPMN elements that can be called by Call Activities (i.e., are CallableElements) are: Process and GlobalTask (see Figure 10.43)."@en ;
                 rdfs:label "CallableElement"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#CancelEvent
:CancelEvent rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Event
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :has_eventDefinition ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :CancelEventDefinition
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :Event ;
             owl:disjointWith :IntermediateCatchEvent ,
                              :IntermediateThrowEvent ,
                              :InterruptingEventSubProcessStartEvent ,
                              :NonEventSubProcessStartEvent ,
                              :NonInterruptingBoundaryEvent ,
                              :NonInterruptingEventSubProcessStartEvent ,
                              :ProcessStartEvent .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#CancelEventDefinition
:CancelEventDefinition rdf:type owl:Class ;
                       rdfs:subClassOf :EventDefinition ;
                       rdfs:comment "Cancel Events are only used in the context of modeling Transaction Sub-Processes (see page 178 for more details on Transactions). There are two variations: a catch Intermediate Event and an End Event."@en ;
                       rdfs:label "CancelEventDefinition"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#CatchEvent
:CatchEvent rdf:type owl:Class ;
            rdfs:subClassOf :Event ;
            rdfs:comment "Events that catch a trigger. All Start Events and some Intermediate Events are catching Events."@en ;
            rdfs:label "CatchEvent"@en .


