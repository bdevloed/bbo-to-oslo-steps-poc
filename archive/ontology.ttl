###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#InputSet
:InputSet rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :has_ioSpecification ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :InputOutputSpecification
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :name ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
