                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( :InputOutputBinding
                                                 :Interface
                                               )
                                 ] ;
                     rdfs:range :CallableElement ;
                     rdfs:label "has_callableElement"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#has_calledElement
:has_calledElement rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :CallActivity ;
                   rdfs:range :CallableElement ;
                   rdfs:comment """The element to be called, which will be either a Process or a
GlobalTask."""@en ;
                   rdfs:label "has_calledElement"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#has_completionCondition
:has_completionCondition rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( :AdHocSubProcess
                                                     :MultiInstanceLoopCharacteristics
                                                   )
                                     ] ;
                         rdfs:range :Expression ;
                         rdfs:comment "This attribute defines a boolean Expression that when evaluated to true, cancels the remaining Activity instances and produces a token."@en ;
                         rdfs:label "has_completionCondition"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#has_complexBehaviorDefinition
:has_complexBehaviorDefinition rdf:type owl:ObjectProperty ;
                               owl:inverseOf :has_multiInstanceLoopCharacteristics ;
                               rdfs:domain :MultiInstanceLoopCharacteristics ;
                               rdfs:range :ComplexBehaviorDefinition .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#has_complexGateway
:has_complexGateway rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :Expression ;
                    rdfs:range :ComplexGateway ;
                    rdfs:comment "[IRIT] references the ComplexGateway which is concerned with the Expression"@en ;
                    rdfs:label "has_complexGateway"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#has_condition
:has_condition rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :ComplexBehaviorDefinition
                                           :ConditionalEventDefinition
                                         )
                           ] ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( :Expression
                                          :FormalExpression
                                        )
                          ] ;
               rdfs:comment "This relation has two source concepts. The definition is related to the source concept: (1) conditionalEventDefinition: The Expression might be underspecified and provided in the form of natural language. For executable Processes (isExecutable = true), if the trigger is Conditional, then a FormalExpression MUST be entered. (2) complexBehaviorDefinition: This attribute defines a boolean Expression that when evaluated to true, cancels the remaining Activity instances and produces a token."@en ;
               rdfs:label "has_condition"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#has_conditionExpression
:has_conditionExpression rdf:type owl:ObjectProperty ;
                         owl:inverseOf :has_sequenceFlow ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain :SequenceFlow ;
                         rdfs:range :Expression ;
                         rdfs:comment """An optional boolean Expression that acts as a gating condition. A token will only
be placed on this Sequence Flow if this conditionExpression evaluates to
true."""@en ;
                         rdfs:label "has_conditionExpression"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#has_container
:has_container rdf:type owl:ObjectProperty ;
               owl:inverseOf :has_flowElements ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :FlowElement ;
               rdfs:range :FlowElementsContainer ;
               rdfs:comment "[IRIT] this relation references the FlowElementContainer that includes the FlowElement"@en ;
               rdfs:label "has_container"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#has_dataResourceInput
:has_dataResourceInput rdf:type owl:ObjectProperty ;
                       rdfs:domain :ThrowEvent ;
                       rdfs:range :DataResource .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#has_dataResourceOutput
:has_dataResourceOutput rdf:type owl:ObjectProperty ;
                        rdfs:domain :CatchEvent ;
                        rdfs:range :DataResource .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#has_default
:has_default rdf:type owl:ObjectProperty ;
             owl:inverseOf :has_defaultElement ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Activity
                                         :ComplexGateway
                                         :ExclusiveGateway
                                         :InclusiveGateway
                                       )
                         ] ;
             rdfs:range :SequenceFlow ;
             rdfs:comment "The Sequence Flow that will receive a token when none of the conditionExpressions on other outgoing Sequence Flows evaluate to true. The default Sequence Flow should not have a conditionExpression. Any such Expression SHALL be ignored."@en ;
             rdfs:label "has_default"@en .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#has_defaultElement
:has_defaultElement rdf:type owl:ObjectProperty ;
                    rdfs:domain :SequenceFlow ;
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( :Activity
                                               :ComplexGateway
                                               :ExclusiveGateway
                                               :InclusiveGateway
                                             )
                               ] .


###  https://www.irit.fr/recherches/MELODI/ontologies/BBO#has_errorRef
:has_errorRef rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :ErrorEventDefinition
                                          :Operation
                                        )
                          ] ;
              rdfs:range :Error ;
              rdfs:comment "for an Operation instance: specifies errors that the Operation may return. An Operation MAY refer to zero or more Error elements."@en ,
                           "for an ErrorEventDefinition instance: If the trigger is an Error, then an Error payload MAY be provided." ;
              rdfs:label "has_errorRef"@en .


